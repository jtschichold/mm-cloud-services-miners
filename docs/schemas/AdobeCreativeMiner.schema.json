{
    "$id": "https://githb.com/jtschichold/mm-cloud-services-miners/AdobeCreativeMiner.schema.json",
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "array",
    "minItems": 1,
    "items": {
        "type": "object",
        "properties": {
            "endpoint": {
                "type": "string",
                "description": "Cloud Service endpoint"
            },
            "serviceName": {
                "type": "string",
                "description": "Name of the Adobe Creative Cloud Services using this endpoint"
            },
            "serviceType": {
                "type": "string",
                "description": "Type of the Adobe Creative Cloud Services using this endpoint"
            }
        },
        "required": ["endpoint", "serviceName", "serviceType"]
    }
}